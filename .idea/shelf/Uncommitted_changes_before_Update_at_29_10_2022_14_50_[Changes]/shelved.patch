+package p4_1.t1_1;
+
+import java.util.Scanner;
+
+public class Main{
+    public static void main(String[] args) {
+        while (true) {
+            System.out.print("\nEnter desired thickness of Math.sinusoid [0(slim) - 64(thicc)]: ");
+            long thickness = new Scanner(System.in).nextInt();
+            if (thickness < 0) { System.out.print("\nEnter positive thickness\n"); }
+            else {
+                System.out.print("\n");
+                char[] gradient = "`^,:;Il!i~+_-?][}{1)(tfjrxnuvczXYUJCLQ0OZmwqpdbkhao*#MW&8%B@$".toCharArray();
+                for (double y = 1 + (double)(thickness + 1) / 100; y >= -1 - (double)(thickness + 1) / 100; y -= Math.PI / 100) {
+                    for (double x = -Math.PI * Math.PI; x <= Math.PI * Math.PI; x += Math.PI / 100) {
+                        double fx = Math.round(1000 * Math.sin(x));
+                        if (Math.abs(fx / (1000 - (y))) < (double)(thickness + 1) / 100) {
+                            int ind = (int) (Math.floor(Math.abs(fx / (1000 - (y))) * 100));
+                            System.out.print(gradient[Math.abs(60 - ind)]);
+                        }
+                        else System.out.print(" ");
+                    }
+                    System.out.print("\n");
+                }
+            }
+        }
+    }
+}
